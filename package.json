{
    "name": "mmmedia-pro",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "main": "dist-electron/main.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "electron:dev": "npm run build:electron && concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:5173 && electron .\"",
        "electron:build": "npm run build && electron-builder",
        "build:electron": "tsc -p electron/tsconfig.json"
    },
    "dependencies": {
        "lucide-react": "^0.462.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "uuid": "^11.0.5",
        "zust and": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "^22.10.5",
        "@types/react": "^19.0.6",
        "@types/react-dom": "^19.0.3",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.2.0",
        "cross-env": "^7.0.3",
        "electron": "^34.0.0",
        "electron-builder": "^25.2.1",
        "eslint": "^9.18.0",
        "eslint-plugin-react": "^7.37.3",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.18",
        "typescript": "~5.7.3",
        "vite": "^5.4.21",
        "vite-plugin-electron": "^0.28.9",
        "vite-plugin-electron-renderer": "^0.14.7",
        "wait-on": "^8.0.2"
    }
}